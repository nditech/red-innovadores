    <%= render '/refinery/header' -%>

    <div class="container">
      <div class="row">
        <div class="span12">
          <%= yield %>
        </div>
      </div>
    </div>
    <footer>
       <div class="container">
         <%= render '/refinery/footer' -%>
      </div>
    </footer>
    <%= render '/refinery/javascripts' %>
    <%= tinymce_assets %>
